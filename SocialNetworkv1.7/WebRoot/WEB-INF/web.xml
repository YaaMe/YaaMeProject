<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoadLeftbarServlet</servlet-name>
    <servlet-class>servlet.LoadLeftbarServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PasswordChangeServlet</servlet-name>
    <servlet-class>servlet.PasswordChangeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>OutputServlet</servlet-name>
    <servlet-class>servlet.OutputServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AdminFindServlet</servlet-name>
    <servlet-class>servlet.AdminFindServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserLoadServlet</servlet-name>
    <servlet-class>servlet.UserLoadServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserFindServlet</servlet-name>
    <servlet-class>servlet.UserFindServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DrawPathServlet</servlet-name>
    <servlet-class>servlet.DrawPathServlet</servlet-class>
  </servlet>








  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoadLeftbarServlet</servlet-name>
    <url-pattern>/loadleftbar.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PasswordChangeServlet</servlet-name>
    <url-pattern>/PasswordChange.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>OutputServlet</servlet-name>
    <url-pattern>/output.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AdminFindServlet</servlet-name>
    <url-pattern>/adminFind.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserLoadServlet</servlet-name>
    <url-pattern>/userload.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserFindServlet</servlet-name>
    <url-pattern>/userFind.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DrawPathServlet</servlet-name>
    <url-pattern>/drawpath.do</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
